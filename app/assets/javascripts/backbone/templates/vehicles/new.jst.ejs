<h1>New vehicle</h1>

<form id="new-vehicle" name="vehicle">
  <div class="field">
    <label for="user_id"> user_id:</label>
    <input type="text" name="user_id" id="user_id" value="<%= user_id %>" >
  </div>

  <div class="field">
    <label for="passenger_space"> passenger_space:</label>
    <input type="text" name="passenger_space" id="passenger_space" value="<%= passenger_space %>" >
  </div>

  <div class="field">
    <label for="rego"> rego:</label>
    <input type="text" name="rego" id="rego" value="<%= rego %>" >
  </div>

  <div class="field">
    <label for="colour"> colour:</label>
    <input type="text" name="colour" id="colour" value="<%= colour %>" >
  </div>

  <div class="field">
    <label for="child_seat"> child_seat:</label>
    <input type="text" name="child_seat" id="child_seat" value="<%= child_seat %>" >
  </div>

  <div class="field">
    <label for="boot"> boot:</label>
    <input type="text" name="boot" id="boot" value="<%= boot %>" >
  </div>

  <div class="field">
    <label for="model_id"> model_id:</label>
    <input type="text" name="model_id" id="model_id" value="<%= model_id %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Create Vehicle" />
  </div>

</form>

